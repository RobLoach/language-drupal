    'body': '/**\n * Implements hook_field_schema().\n */\nfunction $1_field_schema(\$field) {\n  /*\n   *if (\$field[\'type\'] == \'text_long\') {\n   *  \$columns = array(\n   *    \'value\' => array(\n   *      \'type\' => \'text\',\n   *      \'size\' => \'big\',\n   *      \'not null\' => FALSE,\n   *    ),\n   *  );\n   *}\n   *else {\n   *  \$columns = array(\n   *    \'value\' => array(\n   *      \'type\' => \'varchar\',\n   *      \'length\' => \$field[\'settings\'][\'max_length\'],\n   *      \'not null\' => FALSE,\n   *    ),\n   *  );\n   *}\n   *\$columns += array(\n   *  \'format\' => array(\n   *    \'type\' => \'int\',\n   *    \'unsigned\' => TRUE,\n   *    \'not null\' => FALSE,\n   *  ),\n   *);\n   *return array(\n   *  \'columns\' => \$columns,\n   *  \'indexes\' => array(\n   *    \'format\' => array(\'format\'),\n   *  ),\n   *);\n   */\n  \$columns = array(\n    \'${1}\' => array(\n      \'type\' => \'${2}\',\n      \'length\' => ${3},\n      \'unsigned\' => ${4},\n      \'not null\' => ${5},\n    ),\n  );\n  \$columns = array(\n    \'${6}\' => array(\n      \'type\' => \'${7}\',\n      \'length\' => ${8},\n      \'unsigned\' => ${9},\n      \'not null\' => ${10},\n    ),\n  );\n  return array(\n    \'columns\' => \$columns,\n    \'indexes\' => array(\n      \'${11}\' => array(\'${12}\'),\n    ),\n  );\n}'